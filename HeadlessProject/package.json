{
  "name": "magento-headless",
  "homepage": "https://headless.demo.brainvire.dev/",
  "repository": "github:graphcommerce-org/graphcommerce",
  "version": "6.0.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "16.x.x||18.x.x"
  },
  "scripts": {
    "dev": "concurrently -k -n codegen,next 'graphql-codegen -w' 'next dev -p 4001'",
    "codegen": "graphcommerce codegen-config && mesh build && graphql-codegen",
    "build": "graphcommerce codegen-config && mesh build && graphql-codegen && next build && next-sitemap",
    "start": "next start -p 4001",
    "tsc:lint": "tsc --noEmit -p .",
    "lingui": "cross-env NODE_ENV=development lingui extract --clean",
    "postinstall": "is-monorepo '[pkgrun] postinstall' '[pkgrun] patch-package'",
    "create-patch": "patch-package --exclude 'package.json$|gql.ts$|interceptor.tsx$'"
  },
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@graphcommerce/cli": "6.0.0",
    "@graphcommerce/demo-magento-graphcommerce": "6.0.0",
    "@graphcommerce/ecommerce-ui": "6.0.0",
    "@graphcommerce/framer-next-pages": "6.0.0",
    "@graphcommerce/framer-scroller": "6.0.0",
    "@graphcommerce/framer-utils": "6.0.0",
    "@graphcommerce/googleanalytics": "6.0.0",
    "@graphcommerce/googlerecaptcha": "6.0.0",
    "@graphcommerce/googletagmanager": "6.0.0",
    "@graphcommerce/graphcms-ui": "6.0.0",
    "@graphcommerce/graphql": "6.0.0",
    "@graphcommerce/graphql-mesh": "6.0.0",
    "@graphcommerce/image": "6.0.0",
    "@graphcommerce/lingui-next": "6.0.0",
    "@graphcommerce/magento-cart": "6.0.0",
    "@graphcommerce/magento-cart-billing-address": "6.0.0",
    "@graphcommerce/magento-cart-checkout": "6.0.0",
    "@graphcommerce/magento-cart-coupon": "6.0.0",
    "@graphcommerce/magento-cart-email": "6.0.0",
    "@graphcommerce/magento-cart-items": "6.0.0",
    "@graphcommerce/magento-cart-payment-method": "6.0.0",
    "@graphcommerce/magento-cart-shipping-address": "6.0.0",
    "@graphcommerce/magento-cart-shipping-method": "6.0.0",
    "@graphcommerce/magento-category": "6.0.0",
    "@graphcommerce/magento-cms": "6.0.0",
    "@graphcommerce/magento-customer": "6.0.0",
    "@graphcommerce/magento-graphql": "6.0.0",
    "@graphcommerce/magento-newsletter": "6.0.0",
    "@graphcommerce/magento-payment-included": "6.0.0",
    "@graphcommerce/magento-payment-paypal": "6.0.0",
    "@graphcommerce/magento-product": "6.0.0",
    "@graphcommerce/magento-product-bundle": "6.0.0",
    "@graphcommerce/magento-product-configurable": "6.0.0",
    "@graphcommerce/magento-product-downloadable": "6.0.0",
    "@graphcommerce/magento-product-grouped": "6.0.0",
    "@graphcommerce/magento-product-simple": "6.0.0",
    "@graphcommerce/magento-product-virtual": "6.0.0",
    "@graphcommerce/magento-review": "6.0.0",
    "@graphcommerce/magento-search": "6.0.0",
    "@graphcommerce/magento-store": "6.0.0",
    "@graphcommerce/magento-wishlist": "6.0.0",
    "@graphcommerce/next-config": "6.0.0",
    "@graphcommerce/next-ui": "6.0.0",
    "@graphcommerce/react-hook-form": "6.0.0",
    "@lingui/core": "3.17.2",
    "@lingui/react": "3.17.2",
    "@mui/base": "^5.0.0-alpha.126",
    "@mui/lab": "5.0.0-alpha.122",
    "@mui/material": "5.11.12",
    "@next/bundle-analyzer": "^13.4.9",
    "@next/env": "13.2.4",
    "@paypal/react-paypal-js": "^8.0.0",
    "classnames": "^2.2.6",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "framer-motion": "10.2.4",
    "graphql": "16.6.0",
    "html2canvas": "^1.4.1",
    "i18next": "^22.4.14",
    "jarallax": "^1.12.5",
    "jsdom": "^19.0.0",
    "jspdf": "^2.5.1",
    "next": "13.2.4",
    "next-i18next": "^13.2.2",
    "next-pwa": "5.6.0",
    "next-sitemap": "4.0.5",
    "patch-package": "6.5.1",
    "prop-types": "~15.7.2",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-i18next": "^12.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-select": "^5.7.2",
    "react-slick": "^0.28.1",
    "react-tabs": "^6.0.0",
    "sass": "^1.61.0",
    "sharp": "0.31.3",
    "slick-carousel": "^1.8.1",
    "webpack": "5.76.0"
  },
  "devDependencies": {
    "@graphcommerce/eslint-config-pwa": "6.0.0",
    "@graphcommerce/prettier-config-pwa": "6.0.0",
    "@graphcommerce/typescript-config-pwa": "6.0.0",
    "@lingui/cli": "3.17.2",
    "@types/classnames": "^2.2.10",
    "@types/jsdom": "^21.1.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "commit-message-validator": "^1.0.0",
    "eslint": "8.35.0",
    "husky": "4.3.8",
    "prettier": "2.8.4",
    "type-fest": "3.4.0",
    "typescript": "4.9.5"
  },
  "browserslist": [
    "> 1% in alt-EU",
    "not IE 11"
  ],
  "prettier": "@graphcommerce/prettier-config-pwa",
  "eslintConfig": {
    "extends": "@graphcommerce/eslint-config-pwa",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commit-message-validator",
      "pre-push": "yarn build",
      "post-checkout": "rm -rf yarn.lock && yarn && yarn codegen",
      "post-merge": "yarn",
      "post-rewrite": "yarn"
    }
  },
  "config": {
    "commit-message-validator": {
      "_comment": "Validate that all commits contain ticket number.",
      "pattern": "[0-9]+:.+",
      "errorMessage": "Commit message MUST begin with a ticket number, following this format: `46267: Your message here`"
    }
  },
  "resolutions": {
    "pseudolocale": "1.2.0"
  }
}
