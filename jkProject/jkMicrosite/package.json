{
  "name": "brainvire",
  "version": "0.1.0",
  "author": {
    "name": "Brainvire.com"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:watch": "concurrently \"next dev\" \"npm run assets:watch\"",
    "dev:doc:watch": "concurrently \"next dev\" \"npm run assets:watch\" \"npm run doc:start\"",
    "build": "next build",
    "start": "next start",
    "override": "bash setup.bash",
    "server:build": "npm run override && next build",
    "server:monit": "pm2 monit Brainvire.com",
    "server:restart": "pm2 restart pm2.config.js",
    "server:start": "pm2 start pm2.config.js",
    "redeploy:build": "npm run server:build",
    "redeploy:asset": "npm run assets:build",
    "assets:build": "gulp --env production",
    "assets:watch": "gulp watch --env development",
    "clean:install": "rm -rf package-lock.json && rm -rf yarn.lock && rm -rf node_modules && npm install",
    "format": "prettier --write src/**/*.{js,ts,tsx,scss}",
    "lint": "eslint . --ext .ts,.tsx",
    "maintenance:dev": "bash ./deploy/maintenance.dev.sh",
    "maintenance:stage": "bash ./deploy/maintenance.stage.sh",
    "maintenance:prod": "bash ./deploy/maintenance.prod.sh",
    "maintenance:clientstag": "bash ./deploy/maintenance.clientstag.sh",
    "redeploy:maintenance": "npm run redeploy:asset && npm run build",
    "next-lint": "next lint",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && npm run build",
      "pre-push": "npm run build"
    }
  },
  "eslintIgnore": [
    "*.js",
    "*.d.ts"
  ],
  "dependencies": {
    "@react-oauth/google": "^0.11.0",
    "@stripe/stripe-js": "1.32.0",
    "@types/html-to-text": "8.1.1",
    "axios": "0.27.2",
    "cookie": "0.5.0",
    "crypto-js": "4.1.1",
    "js-cookie": "3.0.1",
    "mapbox-gl": "^2.11.0",
    "next": "12.3.1",
    "next-pwa": "5.6.0",
    "next-seo": "5.5.0",
    "node-sass": "6.0.1",
    "nprogress": "0.2.0",
    "os": "^0.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-glider": "4.0.2",
    "react-hook-form": "7.36.1",
    "react-razorpay": "1.2.0",
    "react-redux": "8.0.5",
    "react-select": "5.7.0",
    "react-slick": "^0.29.0",
    "redux": "4.2.0",
    "redux-devtools-extension": "2.13.9",
    "redux-thunk": "2.4.2",
    "stripe": "9.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@types/cookie": "0.5.1",
    "@types/js-cookie": "3.0.2",
    "@types/node": "18.8.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-slick": "^0.23.11",
    "autoprefixer": "10.4.12",
    "compression-webpack-plugin": "10.0.0",
    "concat": "1.0.3",
    "del": "6.1.1",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "4.3.0",
    "gulp-environments": "1.0.1",
    "gulp-flatmap": "1.0.2",
    "gulp-header": "2.0.9",
    "gulp-include": "2.4.1",
    "gulp-optimize-js": "1.1.0",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.1.0",
    "gulp-svgmin": "4.1.0",
    "gulp-terser": "2.1.0",
    "husky": "8.0.1",
    "lazypipe": "1.0.2",
    "require-dir": "1.2.0",
    "sass": "1.45.2",
    "slick-carousel": "^1.8.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1"
  }
}
